{
	"name": "@valeera/nova",
	"version": "0.1.10-alpha2",
	"description": "Web webgpu engine for games or rendering.",
	"main": "build/Nova.js",
	"repository": "ValeeraJS/Nova",
	"jsnext:main": "build/Nova.module.js",
	"module": "build/Nova.module.js",
	"types": "build/src/index.d.ts",
	"files": [
		"build",
		"LICENSE",
		"package.json",
		"README.md",
		"src"
	],
	"directories": {
		"docs": "docs",
		"examples": "examples",
		"src": "src"
	},
	"scripts": {
		"build": "rollup -c scripts/rollup.config.mjs",
		"serve": "http-server",
		"lint": "eslint src --ext js --ext ts",
		"test": "nyc --reporter=html --reporter=text mocha --require esm",
		"travis": "npm run lint && npm test"
	},
	"keywords": [
		"WebGPU"
	],
	"author": "hypnosnova",
	"license": "MIT",
	"devDependencies": {
		"@babel/eslint-parser": "^7.22.15",
		"@rollup/plugin-json": "latest",
		"@rollup/plugin-node-resolve": "latest",
		"@typescript-eslint/eslint-plugin": "latest",
		"@typescript-eslint/parser": "latest",
		"@webgpu/types": "latest",
		"chai": "latest",
		"concurrently": "latest",
		"eslint": "latest",
		"eslint-config-valeera": ">=0.1.4",
		"eslint-plugin-html": "latest",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-prettier": "latest",
		"esm": "latest",
		"http-server": "latest",
		"mocha": "latest",
		"nyc": "latest",
		"prettier": "latest",
		"rollup": "latest",
		"rollup-plugin-typescript2": "latest",
		"typescript": "5.2.2"
	},
	"jspm": {
		"files": [
			"package.json",
			"LICENSE",
			"README.md",
			"build/engine.js",
			"build/engine.min.js",
			"build/engine.module.js"
