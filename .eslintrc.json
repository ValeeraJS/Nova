{
	"root": true,
	"env": {
		"browser": true,
		"node": true,
		"es2023": true
	},
    "parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"extends": ["valeera"],
	"plugins": ["@typescript-eslint", "html", "import"],
	"settings": {
		"polyfills": []
	},
	"globals": {},
	"rules": {
        "@typescript-eslint/no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": true }]
    }
}
